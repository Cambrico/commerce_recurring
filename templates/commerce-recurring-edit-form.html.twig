{#
/**
 * @file
 * Two column template for the recurring edit form.
 *
 * Available variables:
 * - form: The form.
 *
 * @ingroup themeable
 */
#}
<div class="layout-recurring-form clearfix">
  <div class="layout-region layout-region-recurring-main">
    {{ form|without('advanced', 'actions') }}
  </div>
  <div class="layout-region layout-region-recurring-secondary">
    {{ form.advanced }}
  </div>
  <div class="layout-region layout-region-recurring-footer">
    {{ form.actions }}
  </div>
</div>
